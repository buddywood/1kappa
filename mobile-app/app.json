{
  "expo": {
    "name": "1KAPPA",
    "slug": "onekappa-mobile-app",
    "version": "1.1.26",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-logo-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#70110C",
      "dark": {
        "image": "./assets/splash-logo-1024.png",
        "resizeMode": "contain",
        "backgroundColor": "#0D0D0F"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-logo-1024.png",
          "backgroundColor": "#70110C",
          "dark": {
            "image": "./assets/splash-logo-1024.png",
            "resizeMode": "contain",
            "backgroundColor": "#0D0D0F"
          },
          "imageWidth": 300
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "ebillys-websolutions"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onekappa.app",
      "icon": "./assets/icon-ios.icon",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "1KAPPA uses your photo library to let you upload profile photos and share images for events and community posts, such as selecting a picture for your profile or adding photos to an event page.",
        "NSPhotoLibraryAddUsageDescription": "1KAPPA saves images you create or select, such as profile photos or event images, to your photo library only when you choose to do so."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7F4E9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.onekappa.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a43e21f8-fb86-481c-85de-22b723c8e36d"
      }
    }
  }
}
